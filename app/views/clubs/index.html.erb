
<body>
<h1 style="text-align:center">Clubs</h1>



 <div class="container">
    <% @clubs.each do |club| %>
    <div class="tile">
        <p class="profile-pic"> <%= link_to(image_tag(club.pic), club_path(club)) %></p>
        <%= link_to club.name, club_path(club) %>
        
        </div>
    <% end %>
 </div>

</body>