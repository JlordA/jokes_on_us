<% if flash[:my_errors] %>
    <% flash[:my_errors].each do |error| %>
        <p><%= error %></p>
        <% end %>
    <% end %>

<%= form_for(@user) do |f| %>

    



    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :email %><br>
    <%= f.email_field :email %><br>

    <%= f.label "Pic Url" %><br>
    <%= f.text_field :pic %><br>

    <%= f.label :bio %><br>
    <%= f.text_field :bio %><br>

    <%= f.label :age %><br>
    <%= f.number_field :age %><br>

    <%= f.label :hometown %><br>
    <%= f.text_field :hometown %><br>


   
   
    
    <%= f.submit %>
  <% end %>